{
  "name": "Tweep Jumper",
  "version": "1.2",
  "description": "Quickly jump between your top 16 tweeps.",
  "icons": {
    "16": "tj16.png",
    "48": "tj48.png",
    "128": "tj128.png"
  },
  "content_scripts": [{
    "matches": ["http://twitter.com/*", "https://twitter.com/*"],
    "css": ["the.css"],
    "js": ["jquery.js", "the.js"],
    "run_at":  "document_idle"
  }]
}
